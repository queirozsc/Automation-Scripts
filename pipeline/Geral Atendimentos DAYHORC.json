				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "-- Construção tabela agenda Smart\n\nselect top 10\nAGM_PAC as CD_PESSOA_FISICA, \nagm.agm_cnv_cod as CD_CONVENIO,\nemp_cgc as CNPJ,\nCONVERT(DATE,AGM_HINI) AS DATA_AGENDA,\nCONVERT(TIME,AGM_HINI) AS HORA_AGENDA,\nAGM_ATEND AS TIPO_AGENDA,\npac.PAC_NOME AS NM_PACIENTE,\nSMK_NOME AS DS_PROCEDIMENTO_PRINC,\n' ' DS_PROCEDIMENTO_ADIC,\n1 AS IE_QUANTIDADE,\nUSR_NOME as NM_USUARIO,\npsv_nome AS NM_MEDICO,\nEMP_NOME_FANTASIA as SIGLA_UNIDADE,\nEMP_NOME_FANTASIA as MARCA,\nCASE WHEN AGM_STAT = 'A' then 'Aberta'\n     WHEN AGM_STAT = 'E' then 'Executada'      \n     WHEN AGM_STAT = 'C' then 'Cancelada'     \n     WHEN AGM_STAT = 'B' then 'Bloqueada' END STATUS_AGENDA,\n\nconvert(varchar,AGM_OSM_SERIE) + convert(varchar,AGM_OSM_NUM) as NR_ATENDIMENTO,\nAGM_num as NR_SEQUENCIA,\n''ID,\n'' as CD_PROCEDENCIA,\nagm_proc as DS_PROCEDENCIA ,\nagm.AGM_DTMRC as DT_AGENDAMENTO,\nDAY(AGM_HINI) as DIA,\nMONTH(AGM_HINI) as MES,\nYEAR(AGM_HINI) as ANO,\n' ' DS_CLASSIFICACAO,\n' ' DS_AGRUPAMENTO,\nagm.AGM_CANC_MOT_COD as CD_MOTIVO_CANCELAMENTO,\nagm.AGM_CANC_MOT_TIPO as DS_MOTIVO_CANCELAMENTO,\nagm_med as CD_AGENDA\nfrom \nagm, \npac,\ncnv,\nstr,\nsmk,\ngcc,\nusr,\npsv,\nemp\nwhere agm.agm_pac = pac_reg\nand agm_cnv_cod = cnv_cod\nand agm_str_cod = str_cod\nand agm_smk = smk_cod\nand agm_str_cod = gcc_str_cod\nand AGM_USR_LOGIN = USR_LOGIN\nand agm_med = PSV_COD\nand str_emp_cod = emp_cod",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SMART_ITAIGARA_PrdDataSet",
						"type": "DatasetReference"
					}
				],
				"outputs": [
